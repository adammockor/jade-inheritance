{
  "name": "pug-inheritance",
  "description": "Determine the inheritance of Pug templates.",
  "url": "https://github.com/adammockor/pug-inheritance",
  "keywords": [
    "pug",
    "inheritance"
  ],
  "version": "3.0.0-beta.0",
  "bin": {
    "pug-inheritance": "./bin/pug-inheritance"
  },
  "main": "./lib/index",
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "colors": "1.1.2",
    "commander": "2.9.0",
    "glob": "7.1.1",
    "pug-dependency": "~1.0.1",
    "pug-lexer": "^3.0.0",
    "pug-parser": "^2.0.2",
    "pug-walk": "1.1.1",
    "semver": "^5.3.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "~6.24.0",
    "babel-core": "~6.24.0",
    "babel-preset-env": "~1.2.1",
    "tape": "^4.6.3"
  },
  "scripts": {
    "compile": "node_modules/.bin/babel src --out-dir lib",
    "prepublish": "npm run compile",
    "test": "node test/index.js"
  },
  "skipInheritances": [
    "node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adammockor/pug-inheritance.git"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "node": [
              "4.0"
            ]
          }
        }
      ]
    ]
  }
}
